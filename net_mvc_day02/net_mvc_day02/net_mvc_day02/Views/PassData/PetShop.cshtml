
@{
    ViewBag.Title = "PetShop";

}
@model List<string>
<h2>PetShop</h2>
職位:@ViewData["Company"]
<br />
地址:@ViewBag.Address
<br />
<ul>
    @foreach (var pet in Model)
    {
        <li>@pet</li>
    }
</ul>
<h2>PassViewData</h2>
<ul>
    <!--ViewData 有公開GetEnumerator() 方法，故可用foreach 存取ViewData 資料-->
    @foreach (var item in ViewData)
    {
        <li>@item.Key,@item.Value</li>

    }
    <li>Name:@ViewBag.Name</li>
</ul>
<ul>    
    @for (int i = 0; i < Model.Count; i++) {
        <li>@Model[i]</li>
    }
</ul>